  "matching_algorithm": {
    "description": "Fuzzy membership scoring system",
    "steps": [
      "1. For each archetype, check all primary_attributes",
      "2. For numeric attributes, score = 1.0 if in range, gradual falloff outside (trapezoidal)",
      "3. For categorical attributes (damage_pattern), score = 1.0 if match, 0.0 if not",
      "4. For range_constraints, score = 1.0 if champion's range is within bounds",
      "5. Final archetype_score = (sum of attribute scores) / (number of attributes)",
      "6. Threshold: > 0.6 = strong match, 0.4-0.6 = moderate, < 0.4 = weak"
    ],
    "falloff_formula": "score = max(0, 1.0 - (distance_outside_range * penalty_factor))",
    "penalty_factor": 2.0
  }
