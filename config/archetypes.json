{
  "metadata": {
    "version": "1.0.0",
    "description": "Strategic archetype definitions for League of Legends champions. Uses fuzzy membership - champions can belong to multiple archetypes with different scores.",
    "attribute_ranges": {
      "note": "All numeric attributes normalized 0.0-1.0",
      "timing_phases": ["early (0-15min)", "mid (15-25min)", "late (25-40min)"],
      "available_attributes": [
        "cc_score", "mobility_score", "survivability_early", "survivability_mid", 
        "survivability_late", "waveclear_speed", "gold_dependency", "sustain_score",
        "aoe_capability", "dueling_power", "damage_pattern", "range_profile"
      ],
      "pending_attributes": [
        "damage_early", "damage_mid", "damage_late (will be added by timing calculator)"
      ]
    }
  },
