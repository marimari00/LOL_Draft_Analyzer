  "scoring_rules": {
    "fuzzy_scoring": {
      "description": "Each requirement is scored 0-1 based on how well the champion meets it",
      "formula": "For min thresholds: 0 if value < (min - fuzzy_range), 1 if value >= min, linear between. For max thresholds: inverse.",
      "archetype_score": "Weighted average of all requirement scores for that archetype"
    },
    "assignment": {
      "primary_archetype": "Archetype with highest score",
      "secondary_archetype": "Any archetype with score >= 0.7 of primary score",
      "minimum_score": 0.3,
      "fallback_to_specialist": "If no archetype scores >= 0.3"
    }
