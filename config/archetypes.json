  "attribute_definitions": {
    "damage_pattern": {
      "type": "categorical",
      "values": ["burst", "sustained", "poke", "sustained_aoe", "dps"],
      "description": "Primary damage delivery pattern"
    },
    "damage_early": {
      "type": "float",
      "range": [0.0, 1.0],
      "description": "Normalized damage output 0-15 minutes"
    },
    "damage_mid": {
      "type": "float",
      "range": [0.0, 1.0],
      "description": "Normalized damage output 15-25 minutes"
    },
    "damage_late": {
      "type": "float",
      "range": [0.0, 1.0],
      "description": "Normalized damage output 25-40 minutes"
    },
    "survivability_early": {
      "type": "float",
      "range": [0.0, 1.0],
      "description": "Effective HP and threat evasion 0-15 minutes"
    },
    "survivability_mid": {
      "type": "float",
      "range": [0.0, 1.0],
      "description": "Effective HP and threat evasion 15-25 minutes"
    },
    "survivability_late": {
      "type": "float",
      "range": [0.0, 1.0],
      "description": "Effective HP and threat evasion 25-40 minutes"
    },
    "cc_score": {
      "type": "float",
      "range": [0.0, 1.0],
      "description": "Composite: Σ(duration × reliability × AOE × uptime)"
    },
    "mobility_score": {
      "type": "float",
      "range": [0.0, 1.0],
      "description": "Dash count + movement speed modifiers"
    },
    "waveclear_speed": {
      "type": "float",
      "range": [0.0, 1.0],
      "description": "Speed of killing minion waves (AOE damage)"
    },
    "gold_dependency": {
      "type": "float",
      "range": [0.0, 1.0],
      "description": "How much champion power scales with gold"
    },
    "sustain_score": {
      "type": "float",
      "range": [0.0, 1.0],
      "description": "Healing + shielding per second in combat"
    },
    "threat_range": {
      "type": "int",
      "range": [0, 3000],
      "description": "Distance at which champion becomes threatening (units)"
    },
    "effective_ability_range": {
      "type": "int",
      "range": [0, 3000],
      "description": "Maximum poke/engage range of primary abilities"
    },
    "escape_range": {
      "type": "int",
      "range": [0, 1500],
      "description": "Total distance of mobility spells (dashes/blinks)"
    },
    "aoe_capability": {
      "type": "float",
      "range": [0.0, 1.0],
      "description": "AOE damage and CC potential"
    },
    "utility_value": {
      "type": "float",
      "range": [0.0, 1.0],
      "description": "Non-damage value: vision, CC, buffs, map pressure"
    },
    "dueling_power": {
      "type": "float",
      "range": [0.0, 1.0],
      "description": "1v1 fighting strength in side lanes"
    }
  },
  "computation_notes": {
    "fuzzy_membership": "Champions can have partial membership in multiple archetypes. Score = Σ(attribute_matches) / total_attributes",
    "threshold": "Membership > 0.6 is strong, 0.4-0.6 is moderate, < 0.4 is weak",
    "theoretical_fallback": "For champions without pro data, compute attributes from abilities and assign archetypes algorithmically"
  }
}
